{
  "name": "Quater-Hour Notification",
  "description": "This plugin displays ever quater-hour a notification with the current time.",
  "version": "2.8",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "background"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enable / Disable QHN"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
